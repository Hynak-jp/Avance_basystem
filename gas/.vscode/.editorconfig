# EditorConfig helps maintain consistent coding styles
# Placed under gas/.vscode as requested

root = false

[*.{js,ts,tsx,jsx,gs,json}]
indent_style = space
indent_size = 2
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.md]
trim_trailing_whitespace = false

